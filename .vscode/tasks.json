{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo build",
			"type": "shell",
			"command": "cargo",
			"args": ["build"],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
		{
			"label": "cargo check",
			"type": "shell",
			"command": "cargo",
			"args": [
				"check"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "none",
		},
		{
			"label": "cargo test",
			"type": "shell",
			"command": "cargo",
			"args": [
				"test"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
		},
	]
}